{
    "name": "freightorderattach",
    "version": "1.0.0",
    "description": "A simple CAP project.",
    "repository": "<Add your repository here>",
    "license": "UNLICENSED",
    "private": true,
    "dependencies": {
        "@cap-js/attachments": "2.0.2",
        "@cap-js/hana": "^1",
        "@cap-js/sdm": "^1.5.0",
        "@langchain/community": "0.3.39",
        "@langchain/textsplitters": "0.1.0",
        "@sap-ai-sdk/langchain": "1.10.0",
        "@sap-ai-sdk/orchestration": "1.10.0",
        "@sap-cloud-sdk/connectivity": "^3.26.1",
        "@sap-cloud-sdk/http-client": "^3.26.1",
        "@sap-cloud-sdk/resilience": "^3.26.1",
        "@sap/cds": "8.7.2",
        "@sap/xb-msg-amqp-v100": "^0.9.58",
        "@sap/xssec": "^3.6.1",
        "esbuild": "^0.25.8",
        "express": "^4"
    },
    "devDependencies": {
        "@cap-js/cds-types": "^0.9.0",
        "@cap-js/sqlite": "^1",
        "@sap/cds-dk": "^8.1.2"
    },
    "scripts": {
        "start": "cds-serve",
        "watch-freightorderattachui5": "cds watch --open freightorderattachui5/webapp/index.html?sap-ui-xx-viewCache=false",
        "deps": "esbuild --platform=node srv/esm/index.js --bundle --outfile=srv/esm/bundle.js"
    },
    "cds": {
        "log": {
            "levels": {
                "cds.services": "trace"
            }
        },
        "requires": {
            "sdm": {
                "settings": {
                    "repositoryId": "27aeb49e-df08-4868-9b81-c0237ae96543"
                }
            },
            "CE_FREIGHTORDER_0001": {
                "kind": "odata",
                "model": "srv/external/CE_FREIGHTORDER_0001",
                "[production]": {
                    "credentials": {
                        "destination": "ZFO_DESTINATION",
                        "path": "/sap/opu/odata4/sap/api_freightorder/srvd_a2x/sap/freightorder/0001"
                    }
                },
                "[hybrid]": {
                    "credentials": {
                        "destination": "ZFO_DESTINATION",
                        "path": "/sap/opu/odata4/sap/api_freightorder/srvd_a2x/sap/freightorder/0001"
                    }
                }
            },
            "[production]": {
                "messaging": {
                    "kind": "enterprise-messaging",
                    "publishPrefix": "$namespace/",
                    "subscribePrefix": "$namespace/",
                    "queue": {
                        "name": "$namespace/fo1"
                    }
                },
                "db": "hana",
                "auth": "xsuaa"
            },
            "[hybrid]": {
                "messaging": {
                    "kind": "enterprise-messaging-shared",
                    "publishPrefix": "$namespace/",
                    "subscribePrefix": "$namespace/"
                }
            },
            "destinations": true,
            "html5-repo": true,
            "workzone": true
        },
        "sql": {
            "native_hana_associations": false
        }
    },
    "sapux": [
        "app/freightorderattachui5"
    ]
}